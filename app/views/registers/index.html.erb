<div class="container">
  <div class="well">
    <%= link_to "New Product", new_register_path, remote: true, class: "btn btn-primary" %>
  </div>
  <div class="new-product"></div>

<table class="table table-bordered table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>Price</th>
      <th colspan="3">Action</th>
    </tr>
  </thead>

  <tbody class="product-index">
    <%= render "index" %>
  </tbody>
</table>

</div>
<div id="product-modal" class="modal fade"></div>